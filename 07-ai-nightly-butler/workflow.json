      "parameters": {
        "jsCode": "// Unify Inputs\n// Passes either Webhook text or Mock text\nconst items = $input.all();\nconst text = items[0].json.body ? items[0].json.body.text : \"No text input\";\nreturn [{ json: { text } }];"
      },
